{
  "ignore": {
    "useGitignore": true,
    "customPatterns": [
      "**/.git/**",
      "**/.DS_Store",
      
      "**/node_modules/**",
      "**/dist/**",
      "**/build/**",
      "**/coverage/**",
      
      "**/*.lock",
      "**/package-lock.json",
      "**/yarn.lock",
      "**/pnpm-lock.yaml",
      
      "api/.venv/**",
      "api/venv/**",
      "api/__pycache__/**",
      
      "web/.next/**",
      "web/out/**"
    ]
  }
}